{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mlpm": {
      "projectType": "library",
      "root": "projects/mlpm",
      "sourceRoot": "projects/mlpm/src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/mlpm/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mlpm/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mlpm/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/mlpm/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"],
            "karmaConfig": "projects/mlpm/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/mlpm/**/*.ts", "projects/mlpm/**/*.html"],
            "eslintConfig": "projects/mlpm/eslint.config.js"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/mlpm/.storybook",
            "tsConfig": "projects/mlpm/tsconfig.storybook.json",
            "browserTarget": "mlpm:build",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "projects/mlpm"],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/mlpm/.storybook",
            "tsConfig": "projects/mlpm/tsconfig.storybook.json",
            "browserTarget": "mlpm:build",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "projects/mlpm"],
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "packageManager": "pnpm",
    "schematicCollections": ["angular-eslint"]
  }
}
